<div class ="container-fluid">
  <div class ="col-md-12">
    <div class = "section-header">
      <h3>Posts Listing</h3>
    </div>
    <%=link_to "New Post",new_post_path,class:"btn btn-primary float-right",remote: true %>
  </div>
  <div id= "post-listing-section">
    <%= render 'listing',posts:@posts,comment:@comment %>
  </div>
  <%=render 'post_modal' %>
  <%=render 'comments/comment_modal' %>
</div>